public class Main {
	public static void main(String[] args) {
		String str="abc";
	String nstr="";
	for(int i=0;i<str.length();i++){
	  char ch=str.charAt(i);
	  nstr=ch+nstr;
	}
	  if(nstr==str){
	  	System.out.println("Given String Palindrome");
	  }
		else{
			System.out.println("Given String Not Palindrome");
		}
	}
}
