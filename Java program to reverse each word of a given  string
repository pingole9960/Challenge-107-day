import java.util.*;


public class Main {
	public static void main(String[] args) {
		String s="I love programing";
		String or="";
		for(String word:s.split(" ")){
			String rev="";
		for(int i=0;i<word.length();i++){
		    char ch=word.charAt(i);
		    rev=ch+rev;
		}
		or+=rev+" ";
		}
			System.out.print(or);
	}
}
